{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "ter-indent": [
            true,
            4
        ],
        "only-arrow-functions": [
            true,
            "allow-named-functions"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "trailing-comma": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "prefer-const": [
            false
        ],
        "no-console": [
            false
        ]
    },
    "rulesDirectory": []
}